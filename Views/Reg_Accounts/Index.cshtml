@model IEnumerable<mp.Models.Reg_Accounts>

@{
    ViewBag.Title = "Index";
}

@if (Session["Admin_Log"] == null)
{
    Response.Redirect("Admin_Login/Index");
}

<p>
    @Html.ActionLink("Add Employee Account", "Create")
</p>
<h2>Registered Accounts Details</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Borrower_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Phone_Number)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Borrower_Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Borrower_Password)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Account_Created)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Borrower_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Phone_Number)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Borrower_Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Borrower_Password)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Account_Created)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Borrower_Name }) |
                @Html.ActionLink("Details", "Details", new { id = item.Borrower_Name }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Borrower_Name })
            </td>
        </tr>
    }

</table>
